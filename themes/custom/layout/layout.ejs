<!DOCTYPE html>
<html>
<%- partial('_partial/head') %>
<body>
  <div class="container">
    <%- partial('_partial/header') %>
    <section class="content wrap documentationContent">
      <!-- start "body" in layout.ejs -->
      <%- body %>
      <!-- end "body" in layout.ejs -->
      <% if (page.source.split('/')[0] === 'docs') { %>
      <%- partial('_partial/sidebar-docs') %>
      <% } else if (page.source.split('/')[0] === 'about') { %>
      <%- partial('_partial/sidebar-about') %>
      <% } else { %>
      <%- partial('_partial/sidebar-index') %>
      <% } %>
    </section>
    <%- partial('_partial/footer') %>
  </div>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-102241941-1', 'auto');
    ga('send', 'pageview');
  </script>
</body>
</html>
